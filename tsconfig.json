{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "target": "es5",
        "sourceMap": true,
        "lib": ["es6", "es5", "dom"],
        "strictNullChecks": true,
        "outDir": "./dist",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "noUnusedLocals": false,
        "declaration": true,
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ],
    "types": ["pixi.js"]
}
